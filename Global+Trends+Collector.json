    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst collectedAt = new Date().toISOString();\nconst keyword = $('Loop Keywords').item.json.keyword;\nconst keywordCountry = $('Loop Keywords').item.json.country;\nconst keywordCollectedAt = $('Loop Keywords').item.json.keyword_collected_at;\n\nconst results = [];\n\nfor (const item of items) {\n  const data = item.json;\n  \n  // 단일 포스트 객체인 경우\n  if (data.id || data.shortCode) {\n    results.push({\n      json: {\n        collected_at: collectedAt,\n        keyword: keyword,\n        keyword_country: keywordCountry,\n        keyword_collected_at: keywordCollectedAt,\n        post_id: data.id || data.shortCode || '',\n        username: data.ownerUsername || data.owner?.username || '',\n        caption: (data.caption || '').substring(0, 500),\n        likes: data.likesCount || data.likes || 0,\n        comments: data.commentsCount || data.comments || 0,\n        timestamp: data.timestamp || data.takenAtTimestamp || '',\n        url: data.url || (data.shortCode ? 'https://instagram.com/p/' + data.shortCode : '')\n      }\n    });\n  }\n  \n  // 배열인 경우\n  if (Array.isArray(data)) {\n    for (const post of data) {\n      results.push({\n        json: {\n          collected_at: collectedAt,\n          keyword: keyword,\n          keyword_country: keywordCountry,\n          keyword_collected_at: keywordCollectedAt,\n          post_id: post.id || post.shortCode || '',\n          username: post.ownerUsername || post.owner?.username || '',\n          caption: (post.caption || '').substring(0, 500),\n          likes: post.likesCount || post.likes || 0,\n          comments: post.commentsCount || post.comments || 0,\n          timestamp: post.timestamp || post.takenAtTimestamp || '',\n          url: post.url || (post.shortCode ? 'https://instagram.com/p/' + post.shortCode : '')\n        }\n      });\n    }\n  }\n}\n\nif (results.length === 0) {\n  return [{ json: { collected_at: collectedAt, keyword: keyword, keyword_country: keywordCountry, keyword_collected_at: keywordCollectedAt, post_id: 'NO_DATA', username: '', caption: '', likes: 0, comments: 0, timestamp: '', url: '' } }];\n}\n\nreturn results;"
      },
